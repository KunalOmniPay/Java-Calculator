pipeline{
  agent any
  tools{
       maven 'mymaven'
       jdk 'JDK11'
   }
  stages {
    stage('Build'){
       steps {
                withSonarQubeEnv('mysonar') {
                 
                        sh 'mvn clean package sonar:sonar'
                    
                }
            }
     }
    stage('Test'){
      steps{
        sh 'mvn test'
      }
      }
     stage('collect artifact'){
     steps{
     archiveArtifacts artifacts: 'target/*.jar', followSymlinks: false
     }
     }
       
      
  
  }
  }

